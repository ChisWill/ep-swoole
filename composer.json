{
    "name": "chiswill/ep-swoole",
    "description": "Swoole Extension for EP",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "ext-swoole": "*",
        "chiswill/ep": "*"
    },
    "require-dev": {
        "swoole/ide-helper": "~4.5.10"
    },
    "autoload": {
        "psr-4": {
            "Ep\\Swoole\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ep\\Tests\\": "tests",
            "Ep\\Tests\\App\\": "tests/App/src"
        }
    },
    "config": {
        "sort-packages": true
    }
}